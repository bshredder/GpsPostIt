using System;
using System.Data;
using System.Configuration;
using System.Web;
using System.Web.Security;
using System.Web.UI;
using System.Web.UI.WebControls;
using System.Web.UI.WebControls.WebParts;
using System.Web.UI.HtmlControls;

/// <summary>
/// Object used to rate a pin.
/// </summary>
public class RatingToken
{
    private string evaluatorId;
    public string EvaluatorId
    {
        get
        {
            return evaluatorId;
        }
        set
        {
            evaluatorId = value;
        }
    }

    private string ratedPinId;
    public string RatedPinId
    {
        get
        {
            return ratedPinId;
        }
        set
        {
            ratedPinId = value;
        }
    }

    private ushort numStars;
    public ushort NumStars
    {
        get
        {
            return numStars;
        }
        set
        {
            numStars = value;
        }
    }

    public RatingToken()
    {
    }
}
