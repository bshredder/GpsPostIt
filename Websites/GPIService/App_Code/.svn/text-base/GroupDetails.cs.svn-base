using System;
using System.Collections;
using System.Collections.Generic;

/// <summary>
/// Details of a group.
/// </summary>
public class GroupDetails
{
    private string name;
    public string Name
    {
        get
        {
            return name;
        }
        set
        {
            name = value;
        }
    }
    
    public GroupDetails()
    {
    }

    public override string ToString()
    {
        return this.name;
    }
}
